package nju.edu.express.mainui;

import javax.swing.JPanel;
import javax.swing.JTextArea;

import nju.edu.express.logisticsbl.stub.LogisticsBlImpl;
import nju.edu.express.logisticsblservice.LogisticsBlService;

public class MainPanel extends JPanel {

	JTextArea orderID;
	LogisticsBlService logistics;
	
	/**
	 * 初始化Panel
	 */
	void init(){
		
	}
	
	
	
	/**
	 * 获得物流信息界面
	 */
	public void getLogistics(){
		logistics = new LogisticsBlImpl();
		logistics.getLogistics(orderID.getText());
	}
	
	
	
	/**
	 * 返回功能
	 */
	public void back(){
		
	}
	
	
}
