package nju.edu.express.transiterdataservice;

import nju.edu.express.PO.WareInPO;

public interface WareInDataService {

	/**
	 * 根据中转中心ID字段名查找多个持久化对象
	 */
	public WareInPO[] getWareInList(String TransitID);
	
	/**
	 * 根据WareInID 查找单一持久化对象
	 */
	public WareInPO getWareIn(String WareInID);
	
	/**
	 * 增加一个持久化对象
	 */
	public void insertWareIn(WareInPO po);
	
	
}
